/** Add css rules here for your application. */

.icon-tree-joint-o {
  background-image: url("treejo.gif") !important;
}

.icon-tree-joint-c {
  background-image: url("treejc.gif") !important;
}

/* Header rows */
.header {
  border-style: none none none;	
  /*background-color: #2062B8;*/
  background-color: #E2DDCC;
  color: black;  
  font-family:arial;
  /*font-size:18px;*/
  font-size:11px;
  font-weight:extra-bold; 
  /*background-image:url("header_bg.small.gif");*/
  background-position:center top;
  background-repeat:repeat-x;
  /*height:20px;*/
  height:10px;  
  width:100%;
}

.table {
  padding-top:3%;
  background-color:#E7E7E7;  
  border:solid solid solid;  
  margin:none 1px none none;
  /*font:11px arial;*/
  font:10px arial; 
  table-layout: fixed;
}

.table2 {  
  background-color:#E7E7E7;  
  border:solid solid solid;  
  margin:none 1px none none;
  /*font:11px arial;*/
  font:10px arial;
}

.list {
  background-color:white;  
  width:80px;
  /*font:11px arial,tahoma,helvetica,sans-serif;*/
  font:10px arial,tahoma,helvetica,sans-serif;
}

.button {
  background-color:#E2DDCC;
  border:1px outset #F7F5E8;
  /*font:11px arial;*/
  font:8px arial;
  width:50px;
}

.textArea {
  width:80px;
  heigth: 120px;
  font-size:10px;
}

h1 {
  /*font-size: 2em;*/
  font-size: 1em;
  font-weight: bold;
  color: #777777;
  margin: 40px 0px 70px;
  text-align: center;
}

.sendButton {
  display: block;
  /*font-size: 16pt;*/
  font-size: 10pt;
}

/** Most GWT widgets already have a style name defined */
.gwt-DialogBox {
  width: 400px;
}

.dialogVPanel {
  margin: 5px;
}

.serverResponseLabelError {
  color: red;
}

/** Cambiamos el estilo */

.gwt-HorizontalSplitPanel {  
}

.gwt-HorizontalSplitPanel .hsplitter {
  background-color: gray;  
  cursor: move;  
  width: 0px;
}


.gwt-VerticalSplitPanel .vsplitter { 
	background-color: gray;	
    cursor: move; 
    height: 2px;   
}

.gwt-StackPanel .gwt-StackPanelItem {
    border-style: none none none;	
  	background-color: #2062B8;
  	color: black;  
  	font-family:arial;
  	/*font-size:18px;*/
  	font-size:10px;
  	font-weight:extra-bold; 
  	background-image:url("header_bg.small.gif");
  	background-position:center top;
  	background-repeat:repeat-x;
  	height:30px;  
  	width:100%;
}


/** Set ids using widget.getElement().setId("idOfElement") */
#closeButton {
  margin: 15px 6px 6px;
}

/*TAB PANEL*/

.gwt-TabPanelBottom {
  background-color: #E8E8E8;  
}

.gwt-TabBar {  
  background-color: #fff;
}

.gwt-TabBarItem {
  padding: 4px;
  cursor: pointer;
  background-color: #e8e8e8;
  border-bottom: 2px solid white;
  margin-right: 2px;  
  font-family:arial;
  /*font-size:11px;*/
  font-size:10px;
}

.gwt-TabBarItem-selected {
  padding: 4px;
  font-weight: extra-bold;
  cursor: default;
  background-image:url("header_bg.small.gif");
  border-bottom: 2px solid #87ffff;
  margin-right: 2px;
}

